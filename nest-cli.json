{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/data-provider/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/data-provider/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/data-provider",
  "projects": {
    "data-provider": {
      "type": "application",
      "root": "apps/data-provider",
      "entryFile": "main",
      "sourceRoot": "apps/data-provider/src",
      "compilerOptions": {
        "tsConfigPath": "apps/data-provider/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "user-gateway": {
      "type": "application",
      "root": "apps/user-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/user-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-gateway/tsconfig.app.json"
      }
    }
  }
}